  },
  "scripts": {
    "start": "cp .env.$ENV .env.development.local && react-scripts start",
    "start:emulator": "REACT_APP_FIREBASE_EMULATOR=true ENV=dev yarn start",
    "start:dev": "ENV=dev yarn start",
    "start:staging": "ENV=staging yarn start",
    "start:live": "ENV=live yarn start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test-env": "yarn env-cmd -f .env.$ENV yarn test",
    "test:community": "ENV=community yarn test-env",
    "eject": "react-scripts eject",
    "deploy": "git checkout $ENV && git pull && yarn version && env-cmd -f .env.$ENV yarn build && firebase use app-justo-$ENV && firebase deploy",
    "deploy:dev": "ENV=dev yarn deploy",
    "deploy:staging": "ENV=staging yarn deploy",
    "deploy:community": "ENV=community yarn deploy",
    "deploy:live": "ENV=live yarn deploy",
    "deploy:staging:preview": "env-cmd -f .env.staging yarn build && firebase use app-justo-staging && firebase hosting:channel:deploy preview --expires 4d",
    "deploy:live:preview": "env-cmd -f .env.live yarn build && firebase use app-justo-live && firebase hosting:channel:deploy preview --expires 4d",
    "postversion": "git push && git push --tags && git fetch",
    "install-local-types": "yarn remove @appjusto/types && yarn add ../types",
    "cypress:emulator": "cross-env FIREBASE_AUTH_EMULATOR_HOST=localhost:9099 FIRESTORE_EMULATOR_HOST=localhost:8080",
    "cypress:e:open": "yarn cypress:emulator yarn cypress:open",
    "cypress:e:run": "yarn cypress:emulator yarn cypress:run",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "electron:watch": "cross-env NODE_ENV=development tsc -p desktop -w",
    "electron:start": "cross-env NODE_ENV=development electronmon .",
    "electron:pack": "cross-env DEBUG_PROD=true tsc -p desktop && electron-builder --dir",
    "electron:build": "tsc -p desktop && electron-builder",
    "electron:build:beta": "env-cmd -f .env.live yarn build && tsc -p desktop && electron-builder --win portable --x64",
    "electron:build:prod": "env-cmd -f .env.live yarn build && tsc -p desktop && electron-builder -mwl",
    "postinstall": "electron-builder install-app-deps"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "build": {
    "appId": "br.com.appjusto.admin-desktop",
    "productName": "AppJusto Admin Desktop",
    "extends": null,
    "files": [
      "build/desktop/*"
    ],
    "directories": {
      "buildResources": "public"
    },
    "mac": {
      "target": "dmg",
      "category": "public.app-category.business"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "deb",
      "category": "office"
    }
  },
  "devDependencies": {
    "@testing-library/cypress": "^8.0.2",
    "@types/mapbox__polyline": "^1.0.2",
    "@types/react-select": "^4.0.17",
    "cross-env": "^7.0.3",
    "cypress": "9.5.1",
    "cypress-firebase": "^2.2.0",
    "electron": "18.2.3",
    "electron-builder": "^23.1.0",
    "electron-debug": "3.2.0",
    "electron-devtools-installer": "^3.2.0",
    "electronmon": "^2.0.2",
    "eslint-plugin-cypress": "^2.12.1",
    "firebase-admin": "9",
    "msw": "^0.36.3",
    "prettier": "^2.2.1"
  }
}
