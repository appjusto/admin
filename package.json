    "geofirestore": "^4.4.2",
    "geokit": "^1.1.0",
    "github-markdown-css": "^4.0.0",
    "google-map-react": "^2.1.9",
    "i18n-js": "^3.8.0",
    "lodash": "^4.17.20",
    "marked": "^2.0.3",
    "nanoid": "^3.1.20",
    "react": "^17.0.1",
    "react-beautiful-dnd": "^13.0.0",
    "react-chartjs-2": "^3.0.4",
